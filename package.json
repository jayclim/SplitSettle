{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:test": "dotenv -e .env.test.local -- next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "test:db:generate": "dotenv -e .env.test.local -- drizzle-kit generate",
    "test:db:migrate": "dotenv -e .env.test.local -- drizzle-kit migrate",
    "test:setup": "npm run test:db:migrate && npm run test:seed",
    "test:seed": "dotenv -e .env.test.local -- tsx scripts/seed-test-data.ts",
    "test:clean": "dotenv -e .env.test.local -- tsx scripts/clean-test-data.ts",
    "test:reset": "npm run test:clean && npm run test:setup",
    "test": "dotenv -e .env.test.local -- jest --config=jest.config.mjs",
    "test:watch": "dotenv -e .env.test.local -- jest --config=jest.config.mjs --watch"
  },
